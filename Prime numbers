#include<stdio.h>
#include<conio.h>
int main()
 { 
   int i, n;
   int prime = 1;
   n=56;
   for(i = 2; i < n; i++)
    {
      if((n % i) == 0) 
	  {
         prime = 0;
      }
   }
   if (prime == 1)
      printf("%d is prime number.", n);
   else
      printf("%d is not a prime number.", n);
}
